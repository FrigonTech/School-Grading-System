<UserControl x:Class="SchoolGradingSystem.CPU_Usage_Gauge"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SchoolGradingSystem"
             mc:Ignorable="d" Height="34" Width="35" Margin="1,0,0,0">
    <Grid>
        <Ellipse Stroke="Gray" StrokeThickness="5" />
        <Path x:Name="GaugeNeedle" Stroke="Red" StrokeThickness="5" StrokeEndLineCap="Round" />
    </Grid>
</UserControl>
